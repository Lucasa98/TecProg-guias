(define distance2d (lambda (a b)
                     (let ((x1 (car a)) (x2 (car b)) (y1 (cdr a)) (y2 (cdr b))) ;nombres lindos (?)
                       (let ((dx (- x1 x2)) (dy (- y1 y2)))                     ;diferencias
                         (let ((c1 (* dx dx)) (c2 (* dy dy)))                   ;cuadrados
                           (sqrt (+ c1 c2))
                          )
                        )
                      )
                     )
  )

